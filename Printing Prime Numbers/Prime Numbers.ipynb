   "source": [
    "n=int(input(\"n:\"))\n",
    "set1=set(list(range(2,n+1)))\n",
    "set1\n",
    "set2=set()\n",
    "for i in set1:\n",
    "    for j in range(2,i):\n",
    "        if i%j==0:\n",
    "            set2.add(i)\n",
    "\n",
    "set2\n",
    "k=set1.difference(set2)\n",
    "print(list(k),f\"no of prime numbers under {n} is {len(k)}\",sep=\"\\n\")"
   ]
