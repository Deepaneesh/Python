   ],
   "source": [
    "name=input()\n",
    "name1=name.upper()\n",
    "l1=list(name1)\n",
    "dic=dict()\n",
    "for i in l1:\n",
    "    if i in dic:\n",
    "        dic[i]+=1\n",
    "    else:\n",
    "        dic[i]=1\n",
    "print(dic)\n",
    "k1=list(dic.keys())\n",
    "k2=list(dic.values())\n",
    "print(k1)\n",
    "for j in range(len(k1)):\n",
    "    print(f\"the letter {k1[j]} occurs {k2[j]} times in {name}\")"
   ]
